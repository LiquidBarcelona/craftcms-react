{
    "name": "liquidbcn/craftcms-react",
    "description": "Client and Server-side React rendering for Craft CMS",
    "version": "1.0.0",
    "type": "craft-plugin",
    "keywords": ["react", "twig", "craft"],
    "require": {
      "craftcms/cms": "4.8.4",
      "limenius/react-renderer": "^5.0.4",
      "twig/twig": "v3.8.0"
    },
    "autoload": {
      "psr-4": {
        "react\\": "src/"
      }
    },
    "license": "MIT",
    "extra": {
      "handle": "react-in-craft",
      "name": "React for Craft",
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
